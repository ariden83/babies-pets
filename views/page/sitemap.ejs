  <% if (typeof ads !== 'undefined' && ads.length) {
    ads.forEach(ad => { %>
  <url>
    <loc>https://www.babies-pets.fr<%- (ad.url || '') %></loc>
    <lastmod><%- (ad.createdAt ) %></lastmod>
    <changefreq>weekly</changefreq>
    <priority>1.0</priority>
  </url>
  <% });
  }
  if (typeof users !== 'undefined' && users.length) {
  users.forEach(user => { %>
  <url>
    <loc>https://www.babies-pets.fr/mon-compte/profil/<%- (user.url || '') %></loc>
    <lastmod><%= (user.createdAt ? user.createdAt.toISOString() : new Date().toISOString()) %></lastmod>
    <changefreq>weekly</changefreq>
    <priority>1.0</priority>
  </url>
  <% });
  }
  if (typeof especes !== 'undefined' && especes.length) {
  especes.forEach(espece => { %>
  <url>
    <loc>https://www.babies-pets.fr/espece/<%- (espece.type || '') %>/<%- (espece.url || '') %></loc>
    <lastmod><%= (espece.createdAt ? espece.createdAt.toISOString() : new Date().toISOString()) %></lastmod>
    <changefreq>monthly</changefreq>
    <priority>1.0</priority>
  </url>
  <% });
  }
  for (dep in departments) { %>
  <url>
    <loc>https://www.babies-pets.fr/annuaire/elevage/<%- dep %>/<%- departments[dep] %></loc>
    <lastmod>2018-04-22</lastmod>
    <changefreq>weekly</changefreq>
    <priority>1.0</priority>
  </url>
<% } %>
